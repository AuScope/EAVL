# Gives us separation from real DB passwords.
# Production deployment will copy env.properties file over
# this one as passwords should not be in version control.

# For deployments - you can now override the location of this file...
#   <param-name>envPropertiesLocation</param-name>
#   <param-value>file:///c:/env.properties</param-value>
# made possible via Spring3 SpEL in the config.

env.nectar.ec2.accesskey=ENTER_VALUE
env.nectar.ec2.secretkey=ENTER_VALUE

env.aws.ec2.accesskey=ENTER_VALUE
env.aws.ec2.secretkey=ENTER_VALUE

env.oauth2.google.clientid=ENTER_VALUE
env.oauth2.google.clientsecret=ENTER_VALUE

env.db.username=ENTER_VALUE
env.db.password=ENTER_VALUE

env.smtp.username=ENTER_VALUE
env.smtp.password=ENTER_VALUE

env.feedback.email=support@example.com
